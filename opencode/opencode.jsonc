					"limit": {
						"context": 200000,
						"output": 64000
					},
					"cost": {
						"input": 5,
						"output": 25,
						"cache_read": 0.5
					},
					"modalities": {
						"input": [
							"text",
							"image",
							"pdf"
						],
						"output": [
							"text"
						]
					},
					"variants": {
