{
	"$schema": "https://opencode.ai/config.json",
	"theme": "opencode",
	"autoupdate": false,
	"agent": {
		"explore": {
			"disable": true
		},
		"general": {
			"disable": true
		},
		"plan": {
			"model": "opencode/minimax-m2.1-free",
			"permission": {
				"question": "allow"
			}
		}
	},
	"enabled_providers": [
		"anthropic",
		"github-copilot",
		"opencode",
		"quotio",
	],
	"experimental": {
		"disable_paste_summary": true,
	},
	"provider": {
		"quotio": {
			"npm": "@ai-sdk/anthropic",
			"name": "Quotio",
			"options": {
				"baseURL": "http://127.0.0.1:8317/v1"
			},
			"models": {
